import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
 		int x=sc.nextInt();
 		int dec=0;
 		int n=x;
 		int i=0;
		
 		while(n>0)
 		{
           int m=n%10;
		   
 		   dec= dec+(int)m*(int)Math.pow(2,i);
 		   n=n/10;
 		   i++;
 		}
 		System.out.println(dec);

 	}
 }
       

